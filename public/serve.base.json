{
    "headers": [
        {
            "source": "**/*",
            "headers": [
                {
                    "key": "Access-Control-Allow-Origin",
                    "value": "http://localhost"
                },
                {
                    "key": "Cross-Origin-Opener-Policy",
                    "value": "same-origin-allow-popups"
                },
                {
                    "key": "X-Content-Type-Options",
                    "value": "nosniff"
                },
                {
                    "key": "Permissions-Policy",
                    "value": "accelerometer=(), camera=(), geolocation=(), gyroscope=(), magnetometer=(), microphone=(), payment=(), usb=(), fullscreen=(), display-capture=(), midi=(), picture-in-picture=(), publickey-credentials-get=(), screen-wake-lock=(), sync-xhr=(), xr-spatial-tracking=(), autoplay=(), clipboard-read=(), clipboard-write=(), gamepad=()"
                },
		{
			"key": "Content-Security-Policy",
			"value": "default-src 'self'; connect-src 'self'; form-action 'self'; frame-ancestors 'self'; frame-src 'self';" 
		}
            ]
        }
    ]
}

